package com.heo.intro01;

public class FDADFF {
	public static void main(String[] args) {
		
		int[] iEvenOddCountArr = {};
//		iEvenOddCountArr[0] :짝수 갯수, iEvenOddCountArr[1] :홀수 갯수
		
		
		
		
		int iWhileValue = 1;
		while(iWhileValue <= 30) {
			System.out.println(iWhileValue);
			iWhileValue++;
		}

		//짝수 /홀수 판단 
		
		//짝수/홀수 갯수 출력 
		
	}
}
